---- script is working if something goes wrong its probably the game because I only tested on fencing.
---- Replace the animation asset codes with what u want to replace them with different anims

game.Players.LocalPlayer.Character.Animate.climb.ClimbAnim:Destroy()

game.Players.LocalPlayer.Character.Animate.walk.WalkAnim:Destroy()

game.Players.LocalPlayer.Character.Animate.jump.JumpAnim:Destroy()

game.Players.LocalPlayer.Character.Animate.idle.Animation1:Destroy()

game.Players.LocalPlayer.Character.Animate.idle.Animation2:Destroy()

game.Players.LocalPlayer.Character.Animate.sit.SitAnim:Destroy()

game.Players.LocalPlayer.Character.Animate.fall.FallAnim:Destroy()

game.Players.LocalPlayer.Character.Animate.toolnone.ToolNoneAnim:Destroy()


---Now you just replace them all with newly made from scratch animations lol


newanim = Instance.new("Animation")

newanim.Parent = game.Players.LocalPlayer.Character.Animate.idle

newanim.Archivable = true

newanim.AnimationId = "http://www.roblox.com/asset/?id=180435571"

newanim.Name = "Animation1"

newanim:IsA("Animation")

newanim.Archivable = true


newanim1 = Instance.new("Animation")

newanim1.Parent = game.Players.LocalPlayer.Character.Animate.idle

